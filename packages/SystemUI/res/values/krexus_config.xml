<?xml version="1.0" encoding="utf-8"?>

<resources>

    <!-- quick QuickSettings pulldown:
        0: disabled
        1: same side as the clock / signal cluster view (old: right)
        2: opposite of the clock / signal cluster view (old: left)
    -->
    <integer name="config_quick_qs_pulldown">0</integer>

    <!-- Show voice assist affordance on Keyguard -->
    <bool name="config_keyguardShowVoiceAssistAffordance">true</bool>

    <!-- Double tap anywhere in lockscreen makes the device go to sleep -->
    <bool name="config_doubletap_lockscreen_sleep">true</bool>
</resources>
